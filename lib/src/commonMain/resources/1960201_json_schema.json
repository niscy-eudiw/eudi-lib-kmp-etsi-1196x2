    "TrustedEntityService": {
      "type": "object",
      "properties": {
        "ServiceInformation": {
          "$ref": "#/definitions/ServiceInformation"
        },
        "ServiceHistory": {
          "$ref": "#/definitions/ServiceHistory"
        }
