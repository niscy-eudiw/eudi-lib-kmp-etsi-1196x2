    "SchemeName": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/multiLangString"
      },
      "minItems": 1
    },
    "SchemeInformationURI": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/NonEmptyMultiLangURI"
      },
      "minItems": 1
