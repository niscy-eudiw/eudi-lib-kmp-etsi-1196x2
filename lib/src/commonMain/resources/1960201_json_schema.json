    "SchemeTypeCommunityRules": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/NonEmptyMultiLangURI"
      },
      "minItems": 1
    },
    "SchemeTerritory": {
      "type": "string",
      "minLength": 2,
      "maxLength": 2
    },
    "PolicyOrLegalNotice": {
      "type": "array",
      "oneOf": [
        {
          "items": {
            "type": "object",
            "properties": {
              "LoTEPolicy": {
                "$ref": "#/definitions/NonEmptyMultiLangURI"
              }
            },
            "required": [
              "LoTEPolicy"
            ]
          }
        },
        {
          "items": {
            "type": "object",
            "properties": {
              "LoTELegalNotice": {
                "type": "string"
              }
            },
            "required": [
              "LoTELegalNotice"
            ]
          }
        }
      ],
      "minItems": 1
