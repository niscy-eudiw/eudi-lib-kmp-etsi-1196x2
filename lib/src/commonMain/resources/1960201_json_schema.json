    "HistoricalInformationPeriod": {
      "type": "integer"
    },
    "PointersToOtherLoTE": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/OtherLoTEPointer"
      },
      "minItems": 1
    },
    "OtherLoTEPointer": {
      "type": "object",
      "properties": {
        "LoTELocation": {
          "type": "string",
          "format": "uri"
