    "NextUpdate": {
      "type": "string",
      "format": "date-time"
    },
    "DistributionPoints": {
      "type": "array",
      "items": {
        "type": "string",
        "format": "uri"
      },
      "minItems": 1
    },
    "SchemeExtensions": {
      "type": "array",
      "minItems": 1
    },
    "TrustedEntitiesList": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/TrustedEntity"
      },
      "minItems": 1
