    "SchemeOperatorAddress": {
      "type": "object",
      "properties": {
        "SchemeOperatorPostalAddress": {
          "$ref": "#/definitions/PostalAddresses"
        },
        "SchemeOperatorElectronicAddress": {
          "$ref": "#/definitions/ElectronicAddress"
        }
      },
      "required": [
        "SchemeOperatorPostalAddress",
        "SchemeOperatorElectronicAddress"
      ],
      "additionalProperties": false
