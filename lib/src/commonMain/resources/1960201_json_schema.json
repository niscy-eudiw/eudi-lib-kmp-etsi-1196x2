        "X509SubjectNames": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/X509SubjectName"
          },
          "minItems": 1
        },
        "PublicKeyValues": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/PublicKeyValue"
          },
          "minItems": 1
        },
        "X509SKIs": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/X509SKI"
          },
          "minItems": 1
        },
        "OtherIds": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/OtherId"
          },
          "minItems": 1
        },
      "additionalProperties": false
      }
    },
    "pkiOb": {
      "type": "object",
      "properties": {
        "encoding": {
          "type": "string",
          "format": "uri"
        },
        "specRef": {
          "type": "string"
        },
        "val": {
          "type": "string",
          "contentEncoding": "base64"
        }
      },
      "required": [
        "val"
      ],
      "additionalProperties": false
    },
    "X509SubjectName": {
      "type": "string"
    },
    "PublicKeyValue": {
      "$ref": "rfcs/rfc7517.json#/definitions/jwk"
    },
    "X509SKI": {
      "type": "string",
      "contentEncoding": "base64"
    },
    "OtherId": {
      "type": "string"
    },
    "LoTEQualifier": {
      "type": "object",
      "properties": {
        "LoTEType": {
          "$ref": "#/definitions/LoTEType"
        },
        "SchemeOperatorName": {
          "$ref": "#/definitions/SchemeOperatorName"
        },
        "SchemeTypeCommunityRules": {
          "$ref": "#/definitions/SchemeTypeCommunityRules"
        },
        "SchemeTerritory": {
          "$ref": "#/definitions/SchemeTerritory"
        },
        "MimeType": {
          "$ref": "#/definitions/MimeType"
        }
